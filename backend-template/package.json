{
    "name": "om-vsm-backend-template",
    "version": "0.0.1",
    "description": "",
    "author": "",
    "private": true,
    "license": "UNLICENSED",
    "scripts": {
        "build": "nest build",
        "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
        "start": "nest start",
        "start:dev": "nest start --watch",
        "start:debug": "nest start --debug --watch",
        "start:prod": "node dist/main",
        "lint": "eslint \"{src,apps,libs,test}/**/*.ts\"",
        "lint:fix": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
        "test:unit": "jest --config ./test/configs/unit.js",
        "test:unit:file": "node ./scripts/cross-npm-var.js jest --config ./test/configs/unit.js ${npm_config_file}",
        "test:cov": "jest --coverage --config ./test/configs/coverage.js",
        "test:cov:file": "node ./scripts/cross-npm-var.js jest --coverage --config ./test/configs/coverage.js ${npm_config_file}",
        "test:e2e": "jest --config ./test/configs/e2e.js",
        "prepare": "husky",
        "proto:generate": "node ./scripts/cross-npm-var.js protoc --plugin=protoc-gen-ts_proto=./node_modules/.bin/protoc-gen-ts_proto --ts_proto_out=nestJs=true,importSuffix=.js:. --ts_proto_opt=fileSuffix=.proto.type ${npm_config_file}",
        "proto:win:generate": "node ./scripts/cross-npm-var.js protoc --plugin=protoc-gen-ts_proto=%cd%/node_modules/.bin/protoc-gen-ts_proto.cmd --ts_proto_out=nestJs=true,importSuffix=.js:. --ts_proto_opt=fileSuffix=.proto.type ${npm_config_file}"
    },
    "dependencies": {
        "@grpc/grpc-js": "1.11.3",
        "@grpc/proto-loader": "0.7.13",
        "@nestjs/common": "^10.0.0",
        "@nestjs/core": "^10.0.0",
        "@nestjs/cqrs": "10.2.7",
        "@nestjs/microservices": "10.4.4",
        "@nestjs/platform-express": "^10.0.0",
        "@nestjs/typeorm": "10.0.2",
        "@ocean-network-express/om-com-protobuf": "2.0.6",
        "@ocean-network-express/om-mdm-protobuf": "0.5.34",
        "@ocean-network-express/om-vsm-protobuf": "file:../protobuf-test/build/chorus/vsm",
        "@sendgrid/mail": "8.1.3",
        "class-transformer": "0.5.1",
        "class-validator": "0.14.1",
        "dotenv": "16.4.5",
        "google-auth-library": "9.14.1",
        "kafkajs": "2.2.4",
        "pg": "8.13.0",
        "reflect-metadata": "^0.2.0",
        "rxjs": "^7.8.1",
        "typeorm": "0.3.20",
        "uuid": "10.0.0"
    },
    "devDependencies": {
        "@commitlint/cli": "19.5.0",
        "@commitlint/config-conventional": "19.5.0",
        "@nestjs/cli": "^10.0.0",
        "@nestjs/schematics": "^10.0.0",
        "@nestjs/testing": "^10.0.0",
        "@types/express": "^4.17.17",
        "@types/jest": "^29.5.2",
        "@types/node": "^20.3.1",
        "@types/supertest": "^6.0.0",
        "@types/uuid": "10.0.0",
        "@typescript-eslint/eslint-plugin": "^8.0.0",
        "@typescript-eslint/parser": "^8.0.0",
        "eslint": "^8.42.0",
        "eslint-config-prettier": "^9.0.0",
        "eslint-plugin-import": "2.30.0",
        "eslint-plugin-prettier": "^5.0.0",
        "husky": "9.1.6",
        "jest": "^29.5.0",
        "lint-staged": "15.2.10",
        "prettier": "^3.0.0",
        "source-map-support": "^0.5.21",
        "supertest": "^7.0.0",
        "ts-jest": "^29.1.0",
        "ts-loader": "^9.4.3",
        "ts-node": "^10.9.1",
        "tsconfig-paths": "^4.2.0",
        "typescript": "^5.1.3"
    }
}
